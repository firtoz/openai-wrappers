{
  "name": "@firtoz/openai-wrappers",
  "version": "1.0.2",
  "description": "Wrappers for completion and chatCompletion. Supports streaming and basic error handling.",
  "scripts": {
    "build": "yarn tsc && echo build complete",
    "release": "yarn build && changeset publish",
    "test": "eslint . && vitest run",
    "vitest": "vitest"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "typescript",
    "openai",
    "text completion",
    "text-davinci-003",
    "chatgpt-api",
    "gpt-3.5-turbo"
  ],
  "files": [
    "lib/*",
    "src/*"
  ],
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@types/eslint": "^8.40.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.1.3",
    "vitest": "^0.32.0"
  },
  "packageManager": "yarn@3.4.1",
  "dependencies": {
    "axios": "^1.4.0",
    "openai": "^3.3.0"
  },
  "resolutions": {
    "axios": "^1.3.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firtoz/openai-wrappers.git"
  },
  "bugs": {
